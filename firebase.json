{
  "hosting": [
    {
      "target": "website",
      "public": "public",
      "redirects": [
        {
          "source": "/invite",
          "destination": "https://discordapp.com/api/oauth2/authorize?client_id=526971716711350273&permissions=388161&scope=bot",
          "type": 301
        },
        {
          "source": "/support",
          "destination": "https://discord.gg/ppuppun",
          "type": 301
        }
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "cleanUrls": true,
      "trailingSlash": false
    }
  ]
}